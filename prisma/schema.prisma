datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id    Int     @id @default(autoincrement())
  name  String
  email String  @unique
}

model Stock {
  id         Int     @id @default(autoincrement())
  date       DateTime
  trade_code String
  high       Float
  low        Float
  open       Float
  close      Float
  volume     Int
}
